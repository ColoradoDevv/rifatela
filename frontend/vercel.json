{
  "rewrites": [
    { "source": "/", "destination": "/src/views/index.html" },
    { "source": "/inicio", "destination": "/src/views/index.html" },
    { "source": "/rifas", "destination": "/src/views/rifas.html" },
    { "source": "/login", "destination": "/src/views/auth/login.html" },

    { "source": "/admin", "destination": "/src/views/admin/dashboard.html" },
    { "source": "/dashboard", "destination": "/src/views/admin/dashboard.html" },
    { "source": "/admin/vendedores", "destination": "/src/views/admin/new-sellers.html" },
    { "source": "/admin/new-sellers", "destination": "/src/views/admin/new-sellers.html" },

    { "source": "/ventas", "destination": "/src/views/seller/register-sale.html" },

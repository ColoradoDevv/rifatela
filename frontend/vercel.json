    { "source": "/ganadores", "destination": "/src/views/index.html" },
    { "source": "/soporte", "destination": "/src/views/index.html" },

    { "source": "/css/:path*", "destination": "/src/css/:path*" },
    { "source": "/js/:path*", "destination": "/src/js/:path*" },
    { "source": "/assets/:path*", "destination": "/src/assets/:path*" },
    { "source": "/config/:path*", "destination": "/src/config/:path*" },
    { "source": "/components/:path*", "destination": "/src/components/:path*" },
    { "source": "/views/:path*", "destination": "/src/views/:path*" }
